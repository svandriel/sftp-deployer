{
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/recommended",
    "plugin:jest/recommended",
    "prettier"
  ],
  "env": {
    "browser": false,
    "node": true,
    "es2021": true,
    "jest/globals": true
  },
  "plugins": ["@typescript-eslint", "jest"],
  "rules": {}
}
